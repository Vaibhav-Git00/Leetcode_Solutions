class Solution {
    public int canCompleteCircuit(int[] gas, int[] cost) {
        int totalkharchaofgas=0;
        int totalcostofgas=0;
        for(int num : gas){
            totalkharchaofgas+=num;
        }
        for(int num : cost){
            totalcostofgas+=num;
        }
        if(totalkharchaofgas<totalcostofgas)return -1;
        int total =0;
        int result=0;
        for(int i=0;i<gas.length;i++){
            total+=gas[i]-cost[i];
            if(total<0){
                result=i+1;
                total=0;
            }
        }
        return result;
    }
}