class Solution {
    public List<String> validStrings(int n) {
        List<String>result=new ArrayList<>();
        backtrack(n,"",result);
        return result;
    }
    public void backtrack(int n,String str,List<String>result){
        //stop tab hoga ya base case tab hit hoga jab sari str n length ki cover ho jayegiii 
        if(str.length()==n){
            result.add(str);
            return;
        }
        //1 alwayss 
        backtrack(n,str+"1",result);

        //0 tab jab string me piche wala 0 na ho aur string khali ho tab
        if(str.isEmpty()|| str.charAt(str.length()-1)!= '0'){
            backtrack(n,str+"0",result);
        }
    }
}