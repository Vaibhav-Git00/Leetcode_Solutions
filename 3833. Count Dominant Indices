class Solution {
    public int dominantIndices(int[] nums) {
        int n=nums.length;
        int prefix[]=new int[n];
        prefix[n-1]=nums[n-1];
        int sum=nums[n-1];
        for(int i=n-2;i>=0;i--){
            sum+=nums[i];
            prefix[i]=sum;
        }
        int count=0;
        for(int i=0;i<n-1;i++){
            int first=nums[i];
            int len=n-i-1;
            int avg=(prefix[i]-first)/len;
            if(first>avg)count++;
        }
        return count;
    }
}