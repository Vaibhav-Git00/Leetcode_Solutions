class Solution {
    public boolean kLengthApart(int[] nums, int k) {
        int index=-1;
        for(int i=0;i<nums.length;i++){
            if(nums[i]==1 && index!=-1){
                int len =i-index-1;
                if(len<k)return false;
            }
            if(nums[i]==1)index=i;
            
        }
        return true;
    }
}