class Solution {
    public int[] getNoZeroIntegers(int n) {
        int result[]=new int[2];
        int i=1;
        while (i<n){
            int first=i;
            int second=n-i;
            if((first+second)==n){
                String num1 =String.valueOf(first);
                String num2 =String.valueOf(second);
                if(!num1.contains("0") && !num2.contains("0")){
                    result[0]=first;
                    result[1]=second;
                    break;
                }
            }
            i++;
        }
        return result;
    }
}