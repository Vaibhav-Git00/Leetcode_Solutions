class Solution {
    public void solve(int r1,int c1,int r2,int c2,int dis[][]){
        for(int i=r1;i<=r2;i++){
            for(int j=c1;j<=c2;j++){
                dis[i][j]=dis[i][j]+1;
            }
        }
    }
    public int[][] rangeAddQueries(int n, int[][] queries) {
        int dis[][]=new int[n][n];
        int nr=queries.length;
        int nc=queries[0].length;
        for(int row[]:queries){
            int r1=row[0];
            int c1=row[1];
            int r2=row[2];
            int c2=row[3];
            solve(r1,c1,r2,c2,dis);
        }
        return dis;
    }
}



