class Solution {
    public boolean findSubarrays(int[] nums) {
        HashSet<Integer>st=new HashSet<>();
        for(int i=0;i<nums.length-1;i++){
            int sum =0;
            sum+=nums[i]+nums[i+1];
            if(st.contains(sum)){
                return true;
            }else {
                st.add(sum);
            }
        }
        return false;
    }
}