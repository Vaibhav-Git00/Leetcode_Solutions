class Solution {
    public int minMoves(int target, int maxDoubles) {
        int cnt=0;
        while(target>1){
            if(maxDoubles==0)return cnt=cnt+target-1;
            if(target%2==0 && maxDoubles>0){
                    target=target/2;
                    cnt++;
                    maxDoubles--;     
            }else{
                target-=1;
                cnt++;
            }
        }
        return cnt;
    }
}