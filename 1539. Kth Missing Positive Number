class Solution {
    public int findKthPositive(int[] arr, int k) {
        HashSet<Integer>st=new HashSet<>();
        for(int i:arr){
            st.add(i);
        }
        ArrayList<Integer>miss=new ArrayList<>();
        for(int i=1;i<=10000;i++){
            if(!st.contains(i)){
                miss.add(i);
            }
        }
        return miss.get(k-1);
    }
}