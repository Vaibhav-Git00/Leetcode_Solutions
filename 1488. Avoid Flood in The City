class Solution {
    public int[] avoidFlood(int[] rains) {
        TreeSet<Integer>track=new TreeSet<>();
        HashMap<Integer,Integer>help=new HashMap<>();
        int ans[]=new int[rains.length];
        Arrays.fill(ans,1);
        for(int i=0;i<rains.length;i++){
            if(rains[i]>0){
                ans[i]=-1;
                if(help.containsKey(rains[i])){
                    Integer it=track.higher(help.get(rains[i]));
                    if(it==null)return new int[0];
                    ans[it]=rains[i];
                    track.remove(it);
                }
                help.put(rains[i],i);
            }else{
                if(rains[i]==0){
                    track.add(i);
                }
            }
        }
        return ans;
    }
}