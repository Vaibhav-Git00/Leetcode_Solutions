class Solution {
    public int minEatingSpeed(int[] piles, int h) {
        int low=1;
        int max=Integer.MIN_VALUE;
        for(int num : piles){
            max=Math.max(max,num);
        }
        int high=max;
        int ans=max;
        while(low<=high){
            int mid=low+(high-low)/2;
            long count=0;
            for(int p:piles){
                count+=Math.ceil((double)p/mid);
            }
            if(count<=h){
                ans=mid;
                high=mid-1;
            }else{
                low=mid+1;
            }
        }
        return ans;
    }
}