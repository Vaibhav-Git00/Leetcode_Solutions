class Solution {
    public int minimumBoxes(int[] apple, int[] capacity) {
        Arrays.sort(capacity);
        int cap[]=new int[capacity.length];
        int j=0;
        for(int i=capacity.length-1;i>=0;i--){
            cap[j++]=capacity[i];
        }

        int totalapple=0;
        for(int a:apple){
            totalapple+=a;
        }
        int count=0;
        int sum=0;
        for(int i=0;i<capacity.length;i++){
            sum+=cap[i];
            count++;
            if(sum>=totalapple)break;
        }
        return count;
    }
}