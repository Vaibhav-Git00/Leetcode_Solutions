class Solution {
    public int countElements(int[] nums, int k) {
        Arrays.sort(nums);
        int n=nums.length;
        HashMap<Integer,Integer>mp=new HashMap<>();
        for(int i=0;i<n;i++){
            mp.put(nums[i],n-1-i);
        }
        int count=0;
        for(int i=0;i<n;i++){
            if(mp.get(nums[i])>=k)count++;
        }
        return count;
    }
}