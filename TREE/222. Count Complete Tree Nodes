class Solution {
    public int countNodes(TreeNode root) {
        if(root==null)return 0;
        int count=1;
        Queue<TreeNode>q=new LinkedList<>();
        q.add(root);
        while(!q.isEmpty()){
            TreeNode curr=q.poll();
            if(curr.left!=null){
                q.add(curr.left);
                count++;
            }
            if(curr.right!=null){
                q.add(curr.right);
                count++;
            }
        }
        return count;
    }
}