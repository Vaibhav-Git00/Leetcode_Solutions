class Solution {
    public boolean sorted(int nums[]){
        int n=nums.length;
        for(int i=1;i<n;i++){
            if(nums[i]<nums[i-1])return false;
        }
        return true;
    }
    public int sortPermutation(int[] nums) {
        if(sorted(nums))return 0;
        int min=-1;
        for(int i=0;i<nums.length;i++){
            if(i!=nums[i]){
                min=min&nums[i];
            }
        }
        return (min==Integer.MAX_VALUE)?-1:min;
    }
}