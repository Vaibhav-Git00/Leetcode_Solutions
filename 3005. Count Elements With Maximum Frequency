class Solution {
    public int maxFrequencyElements(int[] nums) {
        HashMap<Integer,Integer>mp=new HashMap<>();
        int max=Integer.MIN_VALUE;
        for(int num : nums){
            mp.put(num,mp.getOrDefault(num,0)+1);
            max=Math.max(max,mp.get(num));
        }
        int sum=0;
        for(int key : mp.keySet()){
            if(mp.get(key)==max){
                sum+=mp.get(key);
            }
        }
        return sum;
    }
}

