class Solution {
    public int[] searchRange(int[] nums, int target) {
        int a =lower(nums,target);
        int b= higher(nums,target);
        return  new int[]{a,b};
    }
    public int lower(int nums[],int target){
        int l=0;
        int r=nums.length-1;
        int ret=-1;
        while(l<=r){
            int mid=(l+r)/2;
            if(nums[mid]==target){
                ret=mid;
                r=mid-1;
            }
            else if(nums[mid]<target){
                l=mid+1;
            }else{
                r=mid-1;
            }
        }
        return ret;
    }
    public int higher(int nums[],int target){
        int l=0;
        int r=nums.length-1;
        int ret=-1;
        while(l<=r){
            int mid=(l+r)/2;
            if(nums[mid]==target){
                ret=mid;
                l=mid+1;
            }
            else if(nums[mid]<target){
                l=mid+1;
            }else{
                r=mid-1;
            }
        }
        return ret;
    }
    
}