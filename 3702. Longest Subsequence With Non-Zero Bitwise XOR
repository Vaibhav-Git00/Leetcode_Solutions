class Solution {
    public int longestSubsequence(int[] nums) {
        int sum =0;
        boolean flag=true;
        for(int i=0;i<nums.length;i++){
            sum ^=nums[i];
            if(nums[i]!=0)flag= false;
        }
        if(flag)return 0;
        return (sum==0)?nums.length-1:nums.length;
        
    }
}