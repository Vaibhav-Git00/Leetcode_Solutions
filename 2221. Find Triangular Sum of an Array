class Solution {
    public int triangularSum(int[] nums) {
        int n=nums.length;
        int pas[][]=new int[n][n];
        for (int j = 0; j < n; j++) {
            pas[n - 1][j] = nums[j];
        }
        
        for (int i = n - 2; i >= 0; i--) {
            for (int j = 0; j <= i; j++) {
                pas[i][j] = (pas[i + 1][j] + pas[i + 1][j + 1])%10;
            }
        }
        return pas[0][0];
    }
}