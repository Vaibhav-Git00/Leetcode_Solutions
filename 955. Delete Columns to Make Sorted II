class Solution {
    public int minDeletionSize(String[] strs) {
        int n=strs.length;
        int m=strs[0].length();
        int deletion=0;
        boolean deleted[]=new boolean[n];
        for(int i=0;i<m;i++){
            boolean delete=false;
            for(int j=0;j<n-1;j++){
                if(!deleted[j] && strs[j].charAt(i)>strs[j+1].charAt(i)){
                    deletion++;
                    delete=true;
                    break;
                }
            }
            if(delete)continue;
            for(int k=0;k<n-1;k++){
                deleted[k]=deleted[k] || strs[k].charAt(i)<strs[k+1].charAt(i);
            }
        }
        return deletion;
    }
}